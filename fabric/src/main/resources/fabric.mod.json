{
  "schemaVersion": 1,
  "id": "mca",
  "version": "${version}",
  "name": "Minecraft Comes Alive",
  "description": "No more squidward-villagers! Interact with male and female human villagers and raise a family.",
  "authors": [
    "WildBamaBoy", "SheWolfDeadly"
  ],
  "contact": {
    "sources": "https://github.com/Luke100000/minecraft-comes-alive",
    "issues": "https://github.com/Luke100000/minecraft-comes-alive/issues"
  },
  "license": "GNU",
  "icon": "mca.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "mca.fabric.MCAFabric"
    ],
    "client": [
      "mca.fabric.MCAFabricClient"
    ]
  },
  "mixins": [
    "mca.mixin.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.0",
    "java": ">=17",
    "fabric-api-base": ">=0.1.0",
    "fabric-registry-sync-v0": ">=0.1.0",
    "architectury": ">=4.1.32"
  }
}
